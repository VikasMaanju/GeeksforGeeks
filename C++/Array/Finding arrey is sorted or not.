#include <iostream>
using namespace std;
int main() {
  int arr[]={1,3,5,7,8,10,11,12
};
int n=8;
for (int i =1;i<n;i++){
  if(arr[i-1]<=arr[i]){
    continue;
 // Continue will take the 
    }
  else
  cout<<"arrey is not sorted";
  exit(0);
 // Here exit(0)  is used to terminate the program.
  }
  cout<<"arrey is sorted";
  return 0;
}  
    
   
