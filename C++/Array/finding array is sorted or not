#include<iostream>
using namespace std;
bool issorted( int arr[], int n){         
for( int i = 0; i<n; i++){
    for( int j=i+1; j<n;j++){        
        if(arr[j]< arr[i])
        return false;
    }
}
return true;
}
int main()
{
    int arr[]= {1,2,3,4,5,6,7,8,10}, n=9;
    cout<<issorted(arr,n);
    return true;                            //  0 means array is not sorted and 1 means array is sorted 
}
