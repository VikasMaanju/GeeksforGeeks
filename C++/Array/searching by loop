
 //searching unsorted array
#include<iostream>
using namespace std;

int search(int arr[], int n, int x)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i]==x)
        return i;
    }
    return -1;
}
int main()
{
    int num[]={67,88,55,44,33,89,34},x=55;
    cout<<search(num,7,x);
    return 0;

}    
